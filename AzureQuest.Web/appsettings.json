{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "auth0": {
    "enabled": true,
    "domain": "{MyAuth0Domain}.eu.auth0.com",
    "callbackUrl": "https://{MyAzureAppName}.azurewebsites.net/signin-auth0",
    "clientId": "{MyAuth0ClientId}",
    "clientSecret": "{MyAuth0ClientSecret}"
  },
  "AzureQuestAPIAuth": "{\"client_id\":\"{MyAuth0ClientId}\",\"client_secret\":\"{MyAuth0ClientSecret}\",\"audience\":\"{MyAuth0ApiIdentifier}\",\"grant_type\":\"client_credentials\"}"
}
